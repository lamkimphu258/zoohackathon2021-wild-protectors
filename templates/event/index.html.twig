{% extends 'base.html.twig' %}

{% block title %}Wild{% endblock %}

{% block body %}
    {% for event in events %}
        <h1>{{ event.title }}</h1>
        <p>{{ event.content }}</p>
        <p>Contact: <a href="mailto:{{ event.contact }}">{{ event.contact }}</a></p>
        <hr>
    {% endfor %}
{% endblock %}
